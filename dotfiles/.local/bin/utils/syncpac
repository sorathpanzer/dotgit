#!/bin/sh

rsync -avr --delete $HOME/Documentos /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/Imagens /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/Música /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/Vídeos /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/Transferências /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/Syncthing /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/.ssh /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/.themes /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/.icons /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/.var /media/Vault/Documentos/BackHome
rsync -avr --delete $HOME/.config/qutebrowser/bookmarks /media/Vault/Documentos/BackHome/.config/qutebrowser
rsync -avr --delete $HOME/.local/share/qutebrowser /media/Vault/Documentos/BackHome/.local/share
rsync -avr --delete $HOME/.cache/zsh/history /media/Vault/Documentos/BackHome/.cache/zsh
